    @section scripts{
        <script>
            function LoadLogFor(kioskId, hour) {
                $.get("/Log/GetKioskLogFor", { kioskId: kioskId, hour: hour })
                    .done(function (data) {
                        $("#eventList").empty();
                        $.each(data, function (i, data) {
                            var date = new Date(data.CreateDatetime.match(/\d+/)[0] * 1);
                            var body = "<li>" +
                                "<div class='block'>" +
                                "<div class='tags'>" +
                                "<a href='' class='tag'>" +
                                "<span>" + i + "</span>" +
                                "</a >" +
                                "</div >" +
                                "<div class='block_content'>" +
                                "<h2 class='title'>" +
                                "<a>" + data.Message + "</a>" +
                                "</h2 >" +
                                "<div class='byline'>" +
                                "<span>" + date + "</span> توسط <a>" + data.Hostname + "</a>" +
                                "</div >" +
                                "<p class='excerpt'>" + "</p></div ></div ></li > ";
                            $("#eventList").append(body);
                        });
                    });
            }

        </script>
